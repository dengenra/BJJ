.top{
  width: 100%;
  height: 60rpx;
}
.return {
  width: 100%;
  height: 80rpx;
}
.return image{
  width: 8%;
  height: 60rpx;
  display: inline-block;
}
.return .title{
  display: inline-block;
  font-size: 40rpx;
  vertical-align:top;
}
.frame{
  width: 90%;
  height: 800rpx;
  margin: 0 40rpx;
 

}
.frame .title{
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.frame .title view{
  width: 30%;
  height: 1rpx;
  border-bottom: 1rpx solid black;
}
.frame .title text{
  font-size: 50rpx;
  color: red;
}
.frame .title image{
  width: 40%;
  height: 60%;
}
.frame .body{
  font-size: 35rpx;
  width: 100%;
  height: 500rpx;
  line-height: 70rpx;
}