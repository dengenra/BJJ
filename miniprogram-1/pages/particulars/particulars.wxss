
.return {
  width: 100%;
  height: 80rpx;
  margin-top: 60rpx;
}
.return image{
  width: 8%;
  height: 60rpx;
  display: inline-block;
}
.return .title{
  display: inline-block;
  font-size: 40rpx;
  vertical-align:top;
}
.top{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: center;
}
.top image{
   width: 90%;
   height: 90%;
   border-radius: 30rpx;
}
.theme{
  margin-left: 40rpx;
  font-size: 48rpx;
}
.price{
  margin: 10rpx 30rpx;
}
.price .price1{
  font-size: 60rpx;
  color: red;
}
.price .price2{
  font-size: 30rpx;
  color: gray;
  text-decoration: line-through
}
.hui{
  width: 100%;
  height: 20rpx;
  background-color: rgb(243, 243, 243);
  margin-top:40rpx;
}
.experience{
  display: flex;

  margin-top: 30rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid gainsboro;
}
.experience .kuang{
  width: 40%;
  text-align: center;
}
.experience .kuang2{
  width: 60%;
  font-size: 34rpx;
  text-align: center;
  color: grey;
}
.bottom{
  width: 100%;
  height: 140rpx;
  position: fixed;
  bottom: 0rpx;
  box-shadow: 2px 2px 0px  gainsboro inset;
}
.di{
  position: fixed;
  bottom: 10rpx;
  width: 80%;
  height: 100rpx;
  margin-left: 10%;
  padding-top: 15rpx;
  border-radius: 60rpx;
  background-color: rgb(48, 226, 78);
  text-align: center;
  font-size: 45rpx;
  color: white;
}


